{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "company": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "fashion",
        "electronics",
        " beauty",
        " Kids",
        " Beds",
        "sports",
        "luggage"
      ]
    },
    "price": {
      "type": "integer"
    },
    "colors": {
      "type": "component",
      "component": "product-components.color",
      "repeatable": true
    },
    "image": {
      "type": "text"
    }
  }
}
